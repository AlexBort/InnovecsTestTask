    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/wishListFragment">

    <fragment
        android:id="@+id/wishListFragment"
        android:name="com.example.innovecstesttask.ui.fragments.WishListFragment"
        android:label="First Fragment"
        tools:layout="@layout/fragment_first">
        <action
            android:id="@+id/action_listFragment_to_itemFragment"
            app:destination="@id/wishItemFragment" />
    </fragment>

    <fragment
        android:id="@+id/wishItemFragment"
        android:name="com.example.innovecstesttask.ui.fragments.WishItemFragment"
        android:label="Second Fragment"
        tools:layout="@layout/fragment_second" />

</navigation>
